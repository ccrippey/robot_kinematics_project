<StickFigure>:
    Limb:
        id: torso
        x1: root.width / 2
        y1: root.height * 0.4
        x2: root.width / 2
        y2: root.height * 0.7
    
    Limb2Link:
        id: left_arm
        a1: root.height * 0.14
        a2: root.height * 0.12
        theta: 10
        origin_x: root.width / 2
        origin_y: root.height * 0.67
        theta_origin: 250

    Limb2Link:
        id: right_arm
        a1: root.height * 0.14
        a2: root.height * 0.12
        theta: -10
        origin_x: root.width / 2
        origin_y: root.height * 0.67
        theta_origin: 290
    
    Limb2Link:
        id: left_leg
        a1: root.height * 0.15
        a2: root.height * 0.14
        theta: -10
        origin_x: root.width / 2
        origin_y: root.height * 0.4
        theta_origin: 280

    Limb2Link:
        id: right_leg
        a1: root.height * 0.15
        a2: root.height * 0.14
        theta: 10
        origin_x: root.width / 2
        origin_y: root.height * 0.4
        theta_origin: 260
